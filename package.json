{
	"name": "flowbite-svelte-blocks",
	"version": "0.2.14",
	"description": "Flowbite blocks components for Svelte",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://blog.codewithshin.com"
	},
	"bugs": "https://github.com/shinokada/flowbite-svelte-blocks/issues",
	"homepage": "https://github.com/shinokada/flowbite-svelte-blocks",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:package": "svelte-kit sync && svelte-package",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"package:publish": "standard-version && git push --follow-tags origin main && npm run build:package && npm publish ./package"
	},
	"devDependencies": {
		"@playwright/test": "^1.30.0",
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.7.2",
		"@sveltejs/package": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^5.52.0",
		"@typescript-eslint/parser": "^5.52.0",
		"autoprefixer": "^10.4.13",
		"classnames": "^2.3.2",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.6.3",
		"flowbite-svelte": "^0.29.13",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-heros": "^2.6.0",
		"svelte-meta-tags": "^2.6.5",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.2",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"sveltekit",
		"flowbite",
		"flowbite blocks"
	],
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/flowbite-svelte-blocks"
	}
}
